{
   "name": "typeorm-sample",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "type": "commonjs",
   "devDependencies": {
      "@types/bcrypt": "^5.0.2",
      "@types/express": "^5.0.0",
      "@types/node": "^16.11.10",
      "dotenv": "^16.4.7",
      "eslint": "^9.16.0",
      "nodemon": "^3.1.7",
      "ts-node": "^10.9.2",
      "typescript": "^4.5.2"
   },
   "dependencies": {
      "@faker-js/faker": "^9.3.0",
      "@jorgebodega/typeorm-seeding": "^7.1.0",
      "bcrypt": "^5.1.1",
      "body-parser": "^1.19.1",
      "class-validator": "^0.14.1",
      "express": "^4.21.2",
      "morgan": "^1.10.0",
      "pg": "^8.13.1",
      "reflect-metadata": "^0.1.14",
      "tsconfig-paths": "^4.2.0",
      "typeorm": "^0.3.20",
      "typeorm-extension": "^3.6.3"
   },
   "scripts": {
      "build": "tsc",
      "seed": "ts-node src/seed.ts",
      "start":"ts-node src/seed.ts",
      "dev": "npm run build && npm run seed && nodemon -w src -w .env src/server.ts",
      "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
      "typeorm:drop": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:drop --drop-database -d src/data-source.ts",
      "typeorm:migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d src/data-source.ts",
      "typeorm:migrate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d src/data-source.ts"

   }
}
